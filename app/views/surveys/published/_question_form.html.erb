<div class="card">
  <div class="card-block">
    <div class="form-group row">
      <div class="col-sm-10">
        <h4><%= f.object.title %></h4>
      </div>
    </div>

    <%= f.fields_for :choices do |fc| %>
      <%= render "surveys/published/choice_field_#{f.object.type}", fc: fc %>
    <% end %>
  </div>
</div>
